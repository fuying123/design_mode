<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
	android:id="@+id/fmtest"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical">
		<TextView android:layout_marginTop="40dip" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:textSize="30dip" android:text="@string/test_result_with_colon" />
		<TextView android:layout_marginTop="25dip" android:textSize="25dip" android:layout_width="fill_parent" android:id="@+id/calibration"
			android:layout_height="wrap_content"  />
		<TextView android:layout_marginTop="25dip" android:textSize="25dip" android:layout_width="fill_parent" android:id="@+id/_2g_3g_ft"
			android:layout_height="wrap_content" />
		<TextView android:layout_marginTop="25dip" android:textSize="25dip" android:layout_width="fill_parent" android:id="@+id/_4g_ft"
			android:layout_height="wrap_content"  />
		<TextView android:layout_marginTop="25dip" android:textSize="25dip" android:layout_width="fill_parent" android:id="@+id/flag_pcba"
			android:layout_height="wrap_content"  />
		<TextView android:layout_marginTop="25dip" android:textSize="25dip" android:layout_width="fill_parent" android:id="@+id/flag_auto"
			android:layout_height="wrap_content"  />
	</LinearLayout>
</FrameLayout>
